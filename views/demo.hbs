<iframe src="/project-demos/{{alias}}/index.html" width="100%" height="100%" frameborder="0" onload="resizeIframe(this)"></iframe>

    <script>
    function resizeIframe(obj) {
      var height = "innerHeight" in window
        ? window.innerHeight
        : document.documentElement.offsetHeight;
      obj.style.height = height + 'px';
    }
</script>
