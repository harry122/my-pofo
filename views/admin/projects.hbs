 <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4 mt-5">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Projects</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
          <div class="btn-group mr-2">
            <a href="/admin/projects/create" class="btn btn-sm btn-outline-secondary">Create New</a>
            <button type="button" class="btn btn-sm btn-outline-secondary">upload</button>
          </div>
        </div>
      </div>


      <h2>Section title</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Image</th>
              <th>Name</th>
              <th>Alias</th>
              <th>Github</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <tr>
            {{#each projects}}
              <td><img src="/images/dan-gold.jpg" height="200px" width="200px"></td>
              <td><a href="/admin/projects/{{this.alias}}">{{this.name}}</a></td>
              <td><a href="/admin/projects/{{this.alias}}">{{this.name}}</a></td>
              <td><a href="{{this.githubUrl}}">Link</a></td>
            </tr>
             {{/each}}
          </tbody>
        </table>
      </div>
    </main>